							input(type="checkbox" id="wood-chbx")
							label(for="wood-chbx")
								|лес
						.input-group
							input(type="checkbox" id="landscape-chbx")
							label(for="landscape-chbx")
								|ландшафт
						.input-group
							input(type="checkbox" id="horses-chbx")
							label(for="horses-chbx")
