						.input-group
							input(type="checkbox" id="cows-chbx")
							label(for="cows-chbx")
								|коровы
						.input-group
							input(type="checkbox" id="pigs-chbx")
							label(for="pigs-chbx")
								|свиньи
						.input-group
							input(type="checkbox" id="fields-chbx")
							label(for="fields-chbx")
								|поля
